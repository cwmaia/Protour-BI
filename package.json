{
  "name": "protour-locavia",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "typecheck": "tsc --noEmit",
    "db:setup": "ts-node src/scripts/setupDatabase.ts",
    "db:status": "ts-node src/scripts/checkDbStatus.ts",
    "sync:all": "ts-node src/scripts/syncAll.ts",
    "sync:parallel": "ts-node src/scripts/syncParallel.ts",
    "sync:entity": "ts-node src/scripts/syncEntity.ts",
    "sync:status": "ts-node src/scripts/syncStatus.ts",
    "sync:test": "ts-node src/scripts/testBIEndpoints.ts",
    "sync:test-endpoints": "ts-node src/scripts/testApiEndpoints.ts",
    "sync:batched": "ts-node src/scripts/syncBatched.ts",
    "db:create-tables": "ts-node src/scripts/createMissingTables.ts",
    "sync:os": "ts-node src/scripts/syncOs.ts",
    "sync:os-test": "ts-node src/scripts/testOsPagination.ts",
    "sync:os-complete": "ts-node src/scripts/syncOsComplete.ts",
    "sync:os-optimized": "ts-node src/scripts/syncOsOptimized.ts",
    "sync:os-incremental": "ts-node src/scripts/syncOsIncremental.ts",
    "db:create-os-tables": "ts-node src/scripts/createOsSyncTables.ts",
    "monitor": "LOG_SILENT=true LOG_FILE_ONLY=true NODE_ENV=production ts-node src/monitor/syncMonitor.ts",
    "check:api-health": "ts-node src/scripts/testApiHealth.ts",
    "check:os-status": "ts-node src/scripts/checkOsStatus.ts",
    "db:fix-schema": "ts-node src/scripts/fixDatabaseSchema.ts",
    "api:start": "ts-node src/scripts/startApiServer.ts",
    "api:dev": "nodemon --exec ts-node src/scripts/startApiServer.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/node": "^24.0.4",
    "axios": "^1.10.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "mysql2": "^3.14.1",
    "node-cron": "^4.1.1",
    "socket.io": "^4.7.5",
    "typescript": "^5.8.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/cli-progress": "^3.11.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/mysql": "^2.15.27",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "jest": "^30.0.3",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2"
  }
